{
  "name": "snowpack-demo",
  "version": "1.0.0",
  "description": "snowpack 学习与实践",
  "main": "index.js",
  "scripts": {
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged",
    "install": "snowpack install",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "dev": "cid=id snowpack dev",
    "build": "snowpack build",
    "build:bundle": "snowpack build --bundle",
    "start": "npm run dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.prettierignore"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/prettier.js",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coyeah/snowpack-demo.git"
  },
  "keywords": [],
  "author": "Coyeah_chen@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coyeah/snowpack-demo/issues"
  },
  "homepage": "https://github.com/Coyeah/snowpack-demo#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@snowpack/app-scripts-react": "^1.6.0-alpha.0",
    "@snowpack/plugin-react-refresh": "^1.3.0",
    "@snowpack/plugin-webpack": "^2.0.12",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "snowpack": "^2.6.4",
    "snowpack-plugin-import-map": "^1.0.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "antd": "^4.4.3",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "snowpack-plugin-less": "^1.0.3"
  }
}
